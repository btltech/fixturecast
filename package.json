{
  "name": "fixturecast",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "proxy": "node cors-proxy.js",
    "start": "npm-run-all --parallel dev proxy",
    "build": "vite build --mode production",
    "build:prod": "vite build --mode production --force",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "cf:login": "wrangler login",
    "cf:deploy": "wrangler pages deploy dist --project-name fixturecast",
    "cf:secret": "wrangler pages secret put GEMINI_API_KEY --project-name fixturecast"
  },
  "dependencies": {
    "@aws-sdk/client-eventbridge": "^3.896.0",
    "@aws-sdk/client-lambda": "^3.896.0",
    "@aws-sdk/client-scheduler": "^3.896.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "http-proxy-middleware": "^3.0.5",
    "node-fetch": "^3.3.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.1",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.3",
    "jsdom": "^27.0.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^3.2.4",
    "wrangler": "^3.114.14"
  }
}
